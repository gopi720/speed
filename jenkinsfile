@Library('speed') _
pipeline{
  agent any
  tools{
    maven 'maven'
  }
  stages{
    stage("scm checkout"){
      steps{
        gitcheckout(
          branch: "main",
          url: "https://github.com/gopi720/speed.git"
        )
      }
    }
    stage("maven test"){
      steps{
        mavenTest()
      }
    }
  }
}

